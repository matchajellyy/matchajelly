
public class LoopNestingQuiz {

	public static void main(String[] args) {
		
		for(int dan = 2; dan <= 9; dan++) {
			System.out.print(dan + "ë‹¨: ");
			System.out.println(dan );
			for(int hang = 1; hang <= 9; hang++) {
				System.out.printf("%dx%d=%d ", dan, hang, dan * hang);
			}
			
		}

	}

}
